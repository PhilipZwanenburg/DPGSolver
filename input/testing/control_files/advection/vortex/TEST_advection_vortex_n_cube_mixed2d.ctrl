pde_name  advection
pde_spec  steady/vortex

geom_name n-cube
geom_spec xl_yr/unaligned

dimension 2

mesh_generator   n-cube/2d.geo
mesh_format      gmsh
mesh_domain      parametric
mesh_type        mixed
mesh_level       0 6
mesh_path        ../meshes/


# Simulation variables

#solution_extension vortex_poly
solution_extension vortex_poly_b_poly

interp_tp  GL
interp_si  AO
interp_pyr GLL

basis_geom  lagrange
basis_sol   lagrange

geom_representation  isoparametric
geom_blending_tp     gordon_hall
geom_blending_si     szabo_babuska_gen

p_ref    0 3

fe_method 1


# Testing variables

ml_range_test 0 5
p_range_test  0 3
