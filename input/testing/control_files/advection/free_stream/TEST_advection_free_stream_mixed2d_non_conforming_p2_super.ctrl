pde_name  advection
pde_spec  steady/free_stream

geom_name n-cube
geom_spec x_l

dimension 2

mesh_generator   n-cube/2d.geo
mesh_format      gmsh
mesh_domain      parametric
mesh_type        mixed
mesh_level       2 4
mesh_path        ../meshes/


# Simulation variables

solution_extension  p2
test_case_extension non_conforming

interp_tp  GL
interp_si  WSH
interp_pyr GLL

basis_geom  lagrange
basis_sol   lagrange

geom_representation  superparametric
geom_blending_tp     gordon_hall
geom_blending_si     szabo_babuska_gen

p_ref    2 4

fe_method 1


# Testing variables

ml_range_test 2 4
p_range_test  2 4
