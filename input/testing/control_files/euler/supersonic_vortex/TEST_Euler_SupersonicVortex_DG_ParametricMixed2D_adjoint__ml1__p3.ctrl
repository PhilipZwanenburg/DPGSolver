pde_name  euler
pde_spec  steady/supersonic_vortex

geom_name n-cylinder_hollow_section
geom_spec adjoint_consistent/geom_ar_1-0

dimension 2

mesh_generator   n-cylinder_hollow_section/2d.geo
mesh_format      gmsh
mesh_domain      parametric
mesh_type        mixed
mesh_level       1 1
mesh_path        ../meshes/


# Simulation variables
solution_extension m_i=5
test_case_extension adjoint_consistent

interp_tp  GLL
interp_si  WSH
interp_pyr GLL

basis_geom  lagrange
basis_sol   lagrange

geom_representation  superparametric
geom_blending_tp     gordon_hall
geom_blending_si     szabo_babuska_gen

p_ref    3 3

fe_method 1


# Testing variables

ml_range_test 1 1
p_range_test  3 3
