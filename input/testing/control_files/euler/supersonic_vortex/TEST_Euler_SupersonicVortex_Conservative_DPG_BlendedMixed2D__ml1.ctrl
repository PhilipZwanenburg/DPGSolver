pde_name  euler
pde_spec  steady/supersonic_vortex

geom_name n-cylinder_hollow_section
geom_spec NONE

dimension 2

mesh_generator   n-cylinder_hollow_section/2d.geo
mesh_format      gmsh
mesh_domain      blended
mesh_type        mixed
mesh_level       1 1
mesh_path        ../meshes/


# Simulation variables

test_case_extension conservative

interp_tp  GL
interp_si  WSH
interp_pyr GLL

basis_geom  lagrange
basis_sol   lagrange

geom_representation  superparametric
geom_blending_tp     gordon_hall
geom_blending_si     szabo_babuska_gen

p_ref    1 3
p_s_v_p  1
p_s_f_p  1

p_cub_x 2 2
p_cub_p 10 10

p_test_p 3 3

fe_method 4


# Testing variables

ml_range_test 1 2
p_range_test  1 3
