pde_name  euler
pde_spec  steady/supersonic_vortex

geom_name n-cylinder_hollow_section
geom_spec geom_ar_1-0

dimension 2

mesh_generator   n-cylinder_hollow_section/2d.geo
mesh_format      gmsh
mesh_domain      parametric
mesh_type        tri
mesh_level       0 4
mesh_path        ../meshes/


# Simulation variables
solution_extension  transonic_o
test_case_extension inter_tol

interp_tp  GLL
interp_si  AO
interp_pyr GLL

basis_geom  bezier
basis_sol   lagrange

geom_representation  superparametric
geom_blending_tp     gordon_hall
geom_blending_si     szabo_babuska_gen

p_ref    1 1
p_cub_p  0 0

fe_method 1


# Testing variables

ml_range_test 0 4 // possibly requires start on ml 1 for p1
p_range_test  1 1
