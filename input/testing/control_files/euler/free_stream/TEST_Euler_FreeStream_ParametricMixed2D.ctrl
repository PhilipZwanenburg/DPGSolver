pde_name  euler
pde_spec  steady/free_stream

geom_name n-cube
geom_spec x_lr

dimension 2

mesh_generator   n-cube/2d.geo
mesh_format      gmsh
mesh_domain      parametric
mesh_type        mixed
mesh_level       0 5
mesh_path        ../meshes/


# Simulation variables

solution_extension subsonic
test_case_extension subsonic

interp_tp  GL
interp_si  WSH
interp_pyr GLL

basis_geom  lagrange
basis_sol   lagrange

geom_representation superparametric
geom_blending_tp    gordon_hall
geom_blending_si    szabo_babuska_gen

p_ref    0 2

fe_method 1


# Testing variables

ml_range_test 0 4
p_range_test  0 2
